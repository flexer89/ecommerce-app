{
    "version": 3,
    "timeout": "3s",
    "endpoints": [
      {
        "endpoint": "/api/products/{get1}",
        "input_headers": [
          "*"
        ],
        "method": "GET",
        "output_encoding": "no-op",
        "backend": [
          {
            "url_pattern": "/{get1}",
            "encoding": "no-op",
            "sd": "static",
            "method": "GET",
            "host": [
              "http://products-service:5000"
            ],
            "disable_host_sanitize": false
          }
        ]
      }
    ],
    "output_encoding": "json"
  }